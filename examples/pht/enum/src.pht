((enum Colors [^Object]
    (ector [[r ^int] [g ^int] [b ^int]]
        (ccall)
        (set this/red r)
        (set this/green g)
        (set this/blue b))
    (fld [
        [red    ^int]
        [green  ^int]
        [blue   ^int]])
    (efld [
        [RED    255 0 0]
        [GREEN  0 255 0]
        [BLUE   0 0 255]]))
 (app-fn
    (println ^Colors/RED)))